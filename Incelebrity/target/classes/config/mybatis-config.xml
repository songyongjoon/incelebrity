<?xml version="1.0" encoding="UTF-8"?>
<!-- VO를 접근할 때 간단한 별칭을 주기 위해 만드는 파일임 -->
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.spring.client.review.vo.ReviewVO" alias="review"/>
		<typeAlias type="com.spring.client.reviewReply.vo.ReviewReplyVO" alias="reply"/>
		<typeAlias type="com.spring.admin.profit.vo.ProfitVO" alias="profit"/>
	</typeAliases>
</configuration>
